{
  "name": "nextjs",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "format": "gsmlg-scripts format src pages server.js next.config.js jest.config.js jsconfig.json",
    "dev": "next dev",
    "dev:srv": "NODE_ENV='development' node server.js",
    "build": "NODE_ENV='production' next build && next export -o build",
    "start": "next start",
    "lint": "gsmlg-scripts lint pages src",
    "test": "gsmlg-scripts test",
